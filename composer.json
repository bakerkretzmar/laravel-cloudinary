{
    "name": "bakerkretzmar/laravel-cloudinary",
    "description": "Cloudinary API wrapper for Laravel",
    "keywords": ["laravel", "cloudinary", "api", "image", "upload"],
    "homepage": "https://github.com/bakerkretzmar/laravel-cloudinary",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Dagorn",
            "email": "jeremy.dagorn@gmail.com"
        },
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacobtbk@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Bakerkretzmar\\LaravelCloudinary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bakerkretzmar\\LaravelCloudinary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bakerkretzmar\\LaravelCloudinary\\LaravelCloudinaryServiceProvider"
            ],
            "aliases": {
                "Cloudinary": "Bakerkretzmar\\LaravelCloudinary\\Facades\\Cloudinary"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
